package assignment5;

public abstract class DanielAbstractPerson {
    String firstName;
    String lastName;
    String address;
    int age;
    
    public DanielAbstractPerson() {
      this.firstName = "firstName";
      this.lastName = "lastName";
      this.address = "address";
      this.age = 0;
    }
    
    public DanielAbstractPerson(String firstName, String lastName, String address, int age) {
      this.firstName = firstName;
      this.lastName = lastName;
      this.address = address;
      this.age = age;
    }
    
    public DanielAbstractPerson(String firstName, String lastName) {
      
    }
    
    public String getFirstName() {
       return this.firstName;
    }
    
    public String getLastName() {
      return this.lastName;
    }
    
    public String getFullName() {
      return this.lastName+this.firstName;
    }
    
    public String getAddress() {
      return this.address;
    }
    
    public int getAge() {
      return this.age;
    }
    
    public void setFirstName(String firstName) {
      this.firstName = firstName;
    }
    
    public void setLastName(String lastName) {
      this.lastName = lastName;
    }
    
    public void setAddress(String address) {
      this.address = address;
    }
    
    public void setAge(int age) {
      this.age = age;
    }
    
    public abstract int compareTo(String fullName);
}